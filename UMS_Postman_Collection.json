          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john.doe@example.com\",\n  \"password\": \"SecurePassword123!\",\n  \"mobile\": \"+919876543210\",\n  \"address\": {\n    \"pincode\": \"400001\",\n    \"address\": \"123 Main Street, Apartment 4B\",\n    \"cityInfo\": {\n      \"state\": \"Maharashtra\",\n      \"country\": \"India\"\n    }\n  },\n  \"role\": \"USER\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "auth", "register"]
            }
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.environment.set('userId', response.userId);",
                  "    console.log('User registered with ID:', response.userId);",
                  "}"
                ]
              }
            }
          ]
